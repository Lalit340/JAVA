package com.lp.LOgicTest;

public class StarPattern4 {

	public static void main(String[] args) {
		int i=0,j=0,k=0;
		for(i=1;i<=4;i++) {
			for(j=3;j>=i;j--) {
				System.out.print("-");
			}
			for(k=1;k<=i;k++) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
