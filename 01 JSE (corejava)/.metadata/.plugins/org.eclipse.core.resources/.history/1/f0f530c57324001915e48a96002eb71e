package com.lp.LOgicTest;

public class StarPattern3 {

	public static void main(String[] args) {
		int i=0,j=0;
		for(i=1;i<=10;i++) {
			for(j=1;j<=10;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
		for(i=1;i<=10;i++) {
			for(j=10;j>=i;j--) {
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
